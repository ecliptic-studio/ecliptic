{
  "name": "ecliptic",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "build": "bun build --compile --target bun --outfile server src/index.ts",
    "db:typegen": "bun run kysely-codegen --dialect sqlite --out-file src/db.d.ts",
    "db:better-auth": "bunx @better-auth/cli generate --config src/auth.ts",
    "db:migrate": "bun run scripts/run-migrations.ts && bun run db:typegen",
    "mcp:inspector": "bunx @modelcontextprotocol/inspector"
  },
  "dependencies": {
    "@base-ui-components/react": "^1.0.0-beta.4",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@elysiajs/cors": "^1.4.0",
    "@elysiajs/eden": "^1.4.4",
    "@elysiajs/openapi": "^1.4.11",
    "@elysiajs/static": "^1.4.5",
    "@modelcontextprotocol/sdk": "^1.20.2",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-form": "^1.23.8",
    "@tanstack/react-query": "^5.90.6",
    "@tanstack/react-table": "^8.21.3",
    "@tursodatabase/database": "^0.2.2",
    "better-auth": "^1.3.34",
    "class-variance-authority": "^0.7.1",
    "cloudflare": "^5.2.0",
    "clsx": "^2.1.1",
    "elysia": "^1.4.13",
    "immer": "^10.2.0",
    "kysely": "^0.28.8",
    "kysely-bun-sqlite": "^0.4.0",
    "lucide-react": "^0.546.0",
    "nanoid": "^5.1.6",
    "next-themes": "^0.4.6",
    "node-sql-parser": "^5.3.13",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.5",
    "slug": "^11.0.1",
    "sonner": "^2.0.7",
    "swr": "^2.3.6",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.16",
    "tailwindcss-animate": "^1.0.7",
    "unique-names-generator": "^4.7.1",
    "zod": "^3.25.76",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "bun-plugin-tailwind": "^0.1.2",
    "bun-types": "latest",
    "@better-auth/cli": "^1.3.34",
    "@types/bun": "^1.3.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/slug": "^5.0.9",
    "better-sqlite3": "^12.4.1",
    "kysely-codegen": "^0.19.0"
  },
  "module": "src/index.js"
}
